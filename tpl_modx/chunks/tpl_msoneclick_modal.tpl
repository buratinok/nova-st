
<!-- Modal oneClick-->
<div class="modal msoneclick_modal [[+selector]]_modal fade" id="[[+selector]]" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
     aria-hidden="true">
    <div class="modal-dialog modal-feedback modal-dialog-centered" role="document">
        <div class="modal-content bg-transparent border-0">
            <div class="modal-header border-0">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <img src="assets/templates/nova-st/img/modal-consultation/close.png" height="49" width="49"/>
                </button>
            </div>
            <div class="modal-footer">
                <h4 class="modal-title">[[%msoc]]</h4>
            </div>
            <div class="modal-body [[+selector]]_body">
                
            </div>
        </div>
    </div>
</div>
