
<!-- Modal oneClick-->
<div class="modal msoneclick_modal fade" id="[[+selector]]_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
     aria-hidden="true">
    <div class="modal-dialog modal-feedback modal-dialog-centered" role="document">
        <div class="modal-content  border-0">
            
            <div class="modal-header border-0">
                <h4 class="modal-title my-auto">[[%msoc]]</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <img src="assets/templates/nova-st/img/modal-consultation/close.png" height="49" width="49" alt="close" title="close"/>
                </button>
            </div>
            
            <div class="modal-body" id="[[+selector]]_body"></div>
                
        </div>
    </div>
</div>
